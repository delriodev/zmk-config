
// hold or toggle layer 1   
lower: tap_dance_0 {
    compatible = "zmk,behavior-tap-dance";
    label = "LAYER_TAP_DANCE";
    #binding-cells = <0>;
    tapping-term-ms = <200>;
    bindings = <&mo 1>, <&to 1>;
};

// hold or toggle layer 2
upper: tap_dance_1 {
    compatible = "zmk,behavior-tap-dance";
    label = "UPPER_LAYER_TAP_DANCE";
    #binding-cells = <0>;
    tapping-term-ms = <200>;
    bindings = <&mo 2>, <&to 2>;
};

// key lshift rshift alt
//  (     {     [     <
open1: open1 {
    compatible = "zmk,behavior-mod-morph";
    label = "OPEN1";
    #binding-cells = <0>;
    bindings = <&kp FR_LPRN>, <&kp FR_LCBR>;
    mods = <(MOD_LSFT)>;
};
open2: open2 {
    compatible = "zmk,behavior-mod-morph";
    label = "OPEN2";
    #binding-cells = <0>;
    bindings = <&open1>, <&kp FR_LBRC>;
    mods = <(MOD_RSFT)>;
};
